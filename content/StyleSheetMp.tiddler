<pre>/*{{{*/

[[SvgStyleSheet]]
[[MaloStyleSheet]]

/*needed for mainMenue*/
.noBullets ul{
	list-style: none;
}

/* lower toolbar if not selectet */
.toolbar {
	opacity: 0.3;
}
.selected .toolbar {
	opacity: 1;
}

/* prefer monospace for editing */
.editor textarea, .editor input {
	font-family: 'Consolas';
	font-size: .8em;
	overflow: auto;
}

/*needed to remove toolbar buttons padding*/

.tiddler .toolbar .button, 
.tiddler .toolbar .button:hover,
.tiddler .toolbar .button:active  
{
color: none;
background: none;
padding:0 0 0 0.5em;
margin:0;
border:0;
}


.gray {
	background: gray;
	display: inline-block;
}
/*}}}*/</pre>
